# Habilitar rewrite
RewriteEngine On

# Permitir métodos HTTP
<Limit GET POST>
  Order allow,deny
  Allow from all
</Limit>

# Configurações de upload
php_value upload_max_filesize 100M
php_value post_max_size 100M
php_value memory_limit 512M
php_value max_execution_time 300

# Segurança básica
<Files ~ "^\.">
  Order allow,deny
  Deny from all
</Files>